{
  "name": "nestjs-nextjs-mvc-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cp .env.example .env && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --typescript --apollo",
    "cy": "cypress",
    "scaffold": "cd node_modules/@web/scaffold & cross-env NODE_ENV=production nest start"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/gateway": "^2.11.0",
    "@babel/runtime": "^7.27.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "7.1.1",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.1",
    "@nestjs/apollo": "13.1.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/graphql": "13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/typeorm": "^11.0.0",
    "@preact/signals-core": "^1.9.0",
    "@preact/signals-react": "^3.2.0",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.9.0",
    "commander": "^14.0.0",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "easy-peasy": "^6.1.0",
    "formik": "^2.2.9",
    "google-auth-library": "^9.15.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "graphql": "^16.11.0",
    "nestjs-console": "10.0.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "react": "^19.1.0",
    "react-content-loader": "7.0.2",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-morph": "^26.0.0",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.5",
    "@babel/types": "^7.27.6",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/next": "^9.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/node": "^24.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.34",
    "@types/passport-oauth2": "^1.4.17",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.34.0",
    "connect": "^3.7.0",
    "cookiejar": "^2.1.4",
    "cypress": "^14.4.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "fishery": "^2.3.1",
    "graphql-zeus": "^7.0.7",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^29.7.0",
    "json-schema": "^0.4.0",
    "mime": "^4.0.7",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.8.3"
  }
}
